<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="user_center_text">
        <item name="android:clickable">true</item>
        <item name="android:textSize">18sp</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableRight">@drawable/common_sharp_right</item>
    </style>
    
     <style name="resume_text_one">
         
         <item name="android:paddingTop">10dp</item>
         <item name="android:paddingBottom">10dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">3</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="resume_text_two">
        <item name="android:paddingBottom">10dp</item>
        <item name="android:layout_weight">7</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">3dp</item>
        <item name="android:gravity">right</item>
        <item name="android:drawableBottom">@drawable/resume_content_line</item>
        
    </style>
    
    <style name="regist_right">
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:gravity">right|center_vertical</item>
    </style>

    
    
    <style name="regist_left">
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>
    
</resources>
